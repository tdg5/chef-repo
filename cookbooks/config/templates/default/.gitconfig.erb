[alias]
  amd = commit --amend --no-edit
  amend = commit --amend
  br = branch
  ci = commit
  cm = commit -m
  co = checkout
  get = !git pull && git submodule init && git submodule update
  put = push origin HEAD
  rb = rebase
  rmbr = !sh -c "git branch -d $0 && git push origin :heads/$0 && git remote prune origin"
  rmremote = !sh -c "git push origin :heads/$0"
  st = status
  stat = status
  unci = reset --soft HEAD^
  unstage = reset HEAD
[branch]
  autosetupmerge = true
  autosetuprebase = always
[core]
  excludesfile = <%= node['user']['home_directory'] %>/.gitignore
  whitespace = trailing-space,space-before-tab
[branch "main"]
  rebase = true
[branch "master"]
  rebase = true
[color]
  ui = true
[commit]
  gpgsign = true
[github]
  user = tdg5
[gpg]
  format = ssh
[init]
  defaultBranch = main
[push]
  default = tracking
[rebase]
  autosquash = true
[rerere]
  enabled = true
[user]
  email = <%= node['user']['email'] %>
  name = Danny Guinther
  signingKey = <%= node['user']['home_directory'] %>/.ssh/id_ed25519
[web]
  browser = google-chrome
